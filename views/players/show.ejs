<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PLAYER</title>
    <link rel="stylesheet" href="/styles/main.css" />
</head>
<body>
    <%- include("../partials/playersnav") %>        

    <h1><%= player.name %></h1>
    
    <ul>
        <% player.items.forEach(item => { %>
            
            <li><%= item.name %></li>
        <% }) %>
    </ul>

    <ul>
        <% player.rules.forEach(rule => { %>
            
            <li><%= player.rule.name %></li>
        <% }) %>
    </ul>

    <ul>
        <% rules.forEach(rule => { %>

            <li>Rule #<%= rule.number %></li>
            <li><%= rule.info %></li>
        <% }) %>
    </ul>


</body>
</html>